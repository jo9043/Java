package section2;

public class MyRectangle2 {
	public MyPoint2 lu;
	public MyPoint2 rl;
	
	public MyRectangle2(int x, int y, int w, int h){
		lu = new MyPoint2(x,y);
		width = w;
		height = h;
	}
	
	public MyRectangle2(MyPoint2 p, int w, int h){
		lu = p;
		width = w;
		height = h;
	}
	
	public int calcArea(){
		return (rl.x-lu.x) * (rl.y-lu.y);
	}
	
	public String toString(){
		return "(" + lu.x + "," + lu.y + ") " + (rl.x-lu.x) + " "+ (rl.y-lu.y);
	}
}
