package section1;

/*
 * IS-A 관계
 * A Notebook is a Computer
 * Computer is the superclass of Notebook
 * Notebook is a subclass of Computer
 */

public class Code01 { // 상속
	
	public void run(){
		Notebook test = new Notebook();
		test.manufacturer = "Dell";
		test.processor = "15";
		test.ramSize = 4;
		test.diskSize = 1000;
		test.processorSpeed = 1.2;
		
		test.screenSize = 15.6;
		test.weight = 1.5;
		
		System.out.println(test.computerPower());
		System.out.println(test.toString());
	}
	
	public static void main(String[] args){
		Code01 test = new Code01();
		test.run();
	}

}
